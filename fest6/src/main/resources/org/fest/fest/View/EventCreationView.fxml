        <HBox spacing="10" alignment="CENTER">
            <Label text="eventNameField:" />
            <TextField fx:id="eventNameField" prefWidth="200" />
        </HBox>

        <HBox spacing="10" alignment="CENTER">
            <Label text="descriptionField:" />
            <TextField fx:id="descriptionField" prefWidth="200" />
        </HBox>

        <HBox spacing="10" alignment="CENTER">
            <Label text="startTimeField:" />
            <TextField fx:id="startTimeField" prefWidth="200" />
        </HBox>

        <HBox spacing="10" alignment="CENTER">
            <Label text="endTimeField:" />
            <TextField fx:id="endTimeField" prefWidth="200" />
        </HBox>

        <HBox spacing="10" alignment="CENTER">
            <Label text="placeField:" />
            <TextField fx:id="placeField" prefWidth="200" />
        </HBox>

        <Label text="Create Platinum Ticket:" />
        <HBox spacing="10" alignment="CENTER">
            <Label text="Platinum Ticket Cost:" />
            <TextField fx:id="platinumCostField" prefWidth="200" />
        </HBox>
        <HBox spacing="10" alignment="CENTER">
            <Label text="Number of Platinum Seats:" />
            <TextField fx:id="platinumNOSField" prefWidth="200" />
        </HBox>



        <Label text="Create Gold Ticket:" />
        <HBox spacing="10" alignment="CENTER">
            <Label text="Gold Ticket Cost:" />
            <TextField fx:id="goldNOSField" prefWidth="200" />
        </HBox>
        <HBox spacing="10" alignment="CENTER">
            <Label text="Number of Gold Seats:" />
            <TextField fx:id="silverNOSField" prefWidth="200" />
        </HBox>



        <Label text="Create Silver Ticket:" />
        <HBox spacing="10" alignment="CENTER">
            <Label text="Silver Ticket Cost:" />
            <TextField fx:id="silverCostField" prefWidth="200" />
        </HBox>
        <HBox spacing="10" alignment="CENTER">
            <Label text="Number of Silver Seats:" />
            <TextField fx:id="goldCostField" prefWidth="200" />
        </HBox>

        <Button text="Create Event" onAction="#createEvent" />
    </VBox>

</AnchorPane>





