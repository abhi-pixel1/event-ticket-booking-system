            fx:controller="org.fest.fest.Controller.EventCreationController"
            prefHeight="400.0" prefWidth="600.0">

    <VBox spacing="10" alignment="CENTER" prefHeight="200" prefWidth="600" >

<!--        <HBox spacing="10" alignment="CENTER">-->
